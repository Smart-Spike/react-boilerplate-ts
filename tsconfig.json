{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "tsDist",
    "module": "esnext",
    "target": "es5",
    "rootDir": "app",
    "baseUrl": "app",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["docs", "internals", "node_modules", "tsDist", "dist", "server"]
}
